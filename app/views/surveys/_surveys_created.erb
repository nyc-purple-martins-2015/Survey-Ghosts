
<% questions.each do |question| %>
<p>Question: <%= question.query %><p>
  <%  possible_responses = question.options %>
  <% possible_responses.each do |possible_response| %>
    <% choice = possible_response.response%>
  <p> <%=  choice %> </p>
    <p><%= question.chosen_options.count %></p>
  <% end %>
<% end %>